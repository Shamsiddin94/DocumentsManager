{
    "name": "laravel-enso/documentsmanager",
    "description": "Documents Manager for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "documentsmanager",
        "documents-manager",
        "file-uploader"
    ],
    "homepage": "https://github.com/laravel-enso/DocumentsManager",
    "type": "plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.4.*",
        "laravel-enso/core": "1.2.*",
        "laravel-enso/imagetransformer": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\DocumentsManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\DocumentsServiceProvider"
            ],
            "aliases": {}
        }
    }
}
